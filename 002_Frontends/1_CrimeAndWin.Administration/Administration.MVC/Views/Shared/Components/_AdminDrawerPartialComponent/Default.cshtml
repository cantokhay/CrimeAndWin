<!--begin::Engage drawers-->

<!--begin::Top Drawer-->
@await Component.InvokeAsync("_AdminDrawerTopDrawerPartialComponent") <!--static-->
<!--end::Top Drawer-->

<!--begin::Bottom Drawer-->
@await Component.InvokeAsync("_AdminDrawerBottomDrawerPartialComponent") <!--static-->
<!--end::Bottom Drawer-->

<!--begin::Engage drawers toogle toolbar-->

<div class="engage-toolbar d-flex position-fixed px-5 fw-bolder zindex-2 top-50 end-0 transform-90 mt-20 gap-2">

	<!--begin::Drawer 1 toggle-->
	<button id="kt_engage_demos_toggle" class="engage-demos-toggle btn btn-flex h-35px bg-body btn-color-gray-700 btn-active-color-gray-900 shadow-sm fs-6 px-4 rounded-top-0" title="Top Drawer" data-bs-toggle="tooltip" data-bs-placement="left" data-bs-dismiss="click" data-bs-trigger="hover">
		<span id="kt_engage_demos_label">Drawer 1</span>
	</button>
	<!--end::Drawer 1 toggle-->

	<!--begin::Drawer 2 toggle-->
	<button id="kt_help_toggle" class="engage-help-toggle btn btn-flex h-35px bg-body btn-color-gray-700 btn-active-color-gray-900 shadow-sm px-5 rounded-top-0" title="Bottom Drawer" data-bs-toggle="tooltip" data-bs-placement="left" data-bs-dismiss="click" data-bs-trigger="hover">Drawer 2</button>
	<!--end::Drawer 2 toggle-->

</div>

<!--end::Engage drawers toogle toolbar-->

<!--end::Engage drawers-->